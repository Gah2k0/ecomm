{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-len": [
            "error",
            {
              "code": 150,
              "tabWidth": 2,
              "ignoreComments": true, //"comments": 80
              "ignoreUrls": true,
              "ignoreStrings": true,
              "ignoreTemplateLiterals": true
            }
          ]
    }
}
